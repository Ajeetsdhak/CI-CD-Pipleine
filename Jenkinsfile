 pipeline
	{
		agent any
			
				stages
					{
						stage('print hi')
						{
							steps {
								sh 'echo hi'
								}
						}		
									
						stage('clone hi')
						{
							steps {
								sh 'echo hi'
								}
						}		
					
						stage('test hi')
						{
							steps {
								sh 'echo hi'
								}
						}		
						
						stage('Get approval')
						{
							input 'Should i start build?'
							}	
						
						stage('build hi')
						{
							steps {
								sh 'echo hi'
								}
						}		
			}
	}
